void printArray3D(int[n, m, o] a) {
    for (int i = 0, n) {
        for (int j = 0, m) {
            for (int k = 0, o) {
                printInt(a[i, j, k]);
                if (k != o - 1) printSpaces(1);
            }
            if (j != m - 1) printSpaces(2);
        }
        printNewlines(1);
    }
}

