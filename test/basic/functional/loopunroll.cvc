extern void printInt(int a);
extern void printSpaces(int a);
extern void printNewlines(int a);

export int main() {
    lt();
    le();
    gt();
    ge();
    eq();
    ne();
    return 0;
}

void lt() {
    int i = 5;

    do {
        printInt(i);
        printSpaces(1);
        i = i - 1;
    } while (0 < i);

    printNewlines(1);

    do {
        printInt(i);
        printSpaces(1);
        i = i + 1;
    } while (1 < i);

    printNewlines(1);
}

void le() {
    int i = 5;

    do {
        printInt(i);
        printSpaces(1);
        i = i - 1;
    } while (0 <= i);

    printNewlines(1);

    do {
        printInt(i);
        printSpaces(1);
        i = i + 1;
    } while (1 <= i);

    printNewlines(1);
}

void gt() {
    int i = 0;

    do {
        printInt(i);
        printSpaces(1);
        i = i + 1;
    } while (5 > i);

    printNewlines(1);

    do {
        printInt(i);
        printSpaces(1);
        i = i - 1;
    } while (4 > i);

    printNewlines(1);
}

void ge() {
    int i = 0;

    do {
        printInt(i);
        printSpaces(1);
        i = i + 1;
    } while (5 >= i);

    printNewlines(1);

    do {
        printInt(i);
        printSpaces(1);
        i = i - 1;
    } while (4 >= i);

    printNewlines(1);
}

void eq() {
    int i = 0;

    while (i == 0) {
        printInt(i);
        printSpaces(1);
        i = i + 1;
    }

    printNewlines(1);
}

void ne() {
    int i = 0;

    while (i != 5) {
        printInt(i);
        printSpaces(1);
        i = i + 1;
    }

    printNewlines(1);
}
