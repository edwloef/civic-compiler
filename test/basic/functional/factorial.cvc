extern void printInt(int n);
extern void printNewlines(int n);

export int main() {
    printInt(factorial_recursive(12));
    printNewlines(1);

    printInt(factorial_iterative(12));
    printNewlines(1);

    return 0;
}

int factorial_recursive(int a) {
    if (a == 1) return 1;
    return a * factorial_recursive(a - 1);
}

int factorial_iterative(int a) {
    int b = 1;
    while ((bool)a) {
        b = b * a;
        a = a - 1;
    }
    return b;
}
