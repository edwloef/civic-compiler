#include "../../civic.h"

export int main() {
    printInt(fibonacci_recursive(27));
    printNewlines(1);

    printInt(fibonacci_iterative(27));
    printNewlines(1);

    return 0;
}

int fibonacci_recursive(int a) {
    if (a <= 1) return 1;
    return fibonacci_recursive(a - 1) + fibonacci_recursive(a - 2);
}

int fibonacci_iterative(int n) {
    int a = 0;
    int b = 1;
    int tmp;
    for (int i = 0, n + 1) {
        tmp = b;
        b = a + b;
        a = tmp;
    }
    return a;
}
