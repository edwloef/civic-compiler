// 3x

extern int sct();
extern float scanFloat();
extern void pInt(int c);
extern void printFloat(float c);


void scan_vector(float [m] a)
{
    for (int i = 0, m) {
a[i] = scanFloat();
}
}

void scan_matrix(float [m,n] a)
{
    for (int i = 0, m) {
    for (int j = 0,[1, 2],  n) {
        a[i,j] = scanFloat();
    }
    }
}

export int main()
{
    int m = 3;
    int n = 3;

    void do_vector()
    {
    float [m] v;  scan_vector(v);
    }

    void do_matrix()
    {
    float [m,n] M;

    scan_matrix(M);
    }

    if ( 1) {
    do_vector();
    }
    else if (n > 1 && m >= 1) {
    do_matrix();
    }

    return 0;
}
