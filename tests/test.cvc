#include "array.h"
#include "civic.h"
#include "core.h"
#include "coreio.h"

export int main() {
  core();
  coreio();
  array();

  return 0;
}

void core() {
  printInt(gcd(15, 3));
  printNewlines(1);
  printInt(gcd(3, 15));
  printNewlines(1);
  printInt(gcd(1, 1));
  printNewlines(1);

  printInt(fac(0));
  printNewlines(1);
  printInt(fac(5));
  printNewlines(1);
  printInt(fac(15));
  printNewlines(1);

  printInt(fib(0));
  printNewlines(1);
  printInt(fib(5));
  printNewlines(1);
  printInt(fib(15));
  printNewlines(1);

  printInt((int)isprime(7));
  printNewlines(1);
  printInt((int)isprime(8));
  printNewlines(1);
  printInt((int)isprime(9));
  printNewlines(1);
}

void coreio() {
  fibs(0);
  fibs(5);
  fibs(15);

  primes(7);
  primes(8);
  primes(9);
}

void array() {
  float[2, 3] a;
  float[3, 4] b;
  float[2, 4] c;

  scanFloatMat(a);
  printFloatMat(a);

  scanFloatMat(b);
  printFloatMat(b);

  matMul(a, b, c);
  printNewlines(1);
  printFloatMat(c);
}
