export void fibs(int n) {
    for (int i = 0, n) {
        printInt(fib(n));
        printNewlines(1);
    }
}

export void primes(int n) {
    int a = 0;

    bool isprime() {
        for (int i = 2, a) if (a % i == 0) return false;
        return true;
    }

    for (int b = 0, n) {
        while (!isprime()) a = a + 1;
        printInt(a);
        printNewlines(1);
        a = a + 1;
    }
}
