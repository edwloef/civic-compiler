export int gcd(int a, int b) {
    int c;

    while (b != 0) {
        c = b;
        b = a % b;
        a = c;
    }
    return a;
}

export int fac(int n) {
    int a = 1;

    for (int i = 1, n) a = a * (i + 1);
    return a;
}

export int fib(int n) {
    int a = 0;
    int b = 1;
    int c;

    for (int i = 0, n) {
        c = a;
        a = b;
        b = a + c;
    }
    return a;
}

